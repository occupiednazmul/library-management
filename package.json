{
  "name": "library-management",
  "version": "0.0.0",
  "description": "A library management app.",
  "keywords": [
    "library",
    "library-management"
  ],
  "author": "Md Nazmul Huda",
  "license": "MIT",
  "type": "module",
  "private": "true",
  "workspaces": [
    "api",
    "client"
  ],
  "scripts": {
    "dev:api": "npm run build:client && npm run dev --workspace api",
    "dev:client": "npm run dev --workspace client",
    "build:api": "npm run build --workspace api",
    "build:client": "npm run build --workspace client",
    "build": "npm run build:client && npm run build:api",
    "deploy": "vercel --prod",
    "preview:api": "npm run build && node server",
    "preview:client": "npm run build:client && npm run preview --workspace client"
  }
}
